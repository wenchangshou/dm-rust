{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HS Power Sequencer Config",
  "type": "object",
  "properties": {
    "port_name": {
      "type": "string"
    },
    "baud_rate": {
      "type": "integer",
      "default": 9600
    },
    "device_address": {
      "type": "integer",
      "default": 1
    }
  },
  "required": [
    "port_name"
  ]
}